
#include <stdio.h>
#include <stdint.h>
#include <io.h>
#include <unistd.h>

#define REG_READ_BASE 0x00000000
#define OFFSET 0x00000000

#define REG_READ_SELECT_BASE 0x00000010


int main(void)
{ 
    int reg_read = 0;

    while( 1 ) 
    {
    	for(int i = 0; i < 9; i++){
    		IOWR_8DIRECT(REG_READ_SELECT_BASE,OFFSET,i);
    		reg_read = IORD_32DIRECT(REG_READ_SELECT_BASE,OFFSET);
    		usleep(100000);
    	}



        if( count == 0xFF )
        {
        	usleep(1000000); // If done counting, wait about 1 second...
        	count = 0;
        }

        count++;
    }

    return 0;
}
